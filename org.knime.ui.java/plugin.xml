<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <e4view
            allowMultiple="false"
            class="org.knime.ui.java.browser.KnimeBrowserView"
            icon="icons/knime.png"
            id="org.knime.ui.java.browser.view"
            name="KNIME Web UI"
            restorable="false">
      </e4view>
   </extension>
   <extension
         id="org.knime.ui.java.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.e4.ui.css.swt.theme">
      <theme
            basestylesheeturi="css/web-ui.css"
            id="org.knime.ui.java.theme"
            label="KNIME Web UI">
      </theme>
   </extension>

</plugin>
