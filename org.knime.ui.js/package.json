{
  "name": "knime-ui",
  "private": true,
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && vite build",
    "build:ci": "pnpm build && mv ./dist/sbom/bom.json ./bom.json",
    "build:e2e": "vite build --mode e2e",
    "serve": "vite preview --port 3000",
    "lint": "run-p 'lint:* --fix'",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "ci:lint-format": "run-p lint:* format:check -c",
    "audit": "pnpm audit --prod",
    "test:unit": "vitest",
    "test:e2e": "playwright test behavior/ --workers=4",
    "test:e2e:update": "playwright test behavior/ --update-snapshots --workers=1",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test performance/ --workers=1",
    "coverage": "vitest run --coverage",
    "license-check": "license-check -c",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "add-husky": "cd .. && husky ./org.knime.ui.js/.husky",
    "postinstall": "license-check -o licenses/used-packages.json"
  },
  "dependencies": {
    "@antfu/utils": "^9.1.0",
    "@floating-ui/vue": "^1.1.1",
    "@fontsource/roboto": "4.x",
    "@fontsource/roboto-condensed": "4.x",
    "@fontsource/roboto-mono": "4.x",
    "@knime/components": "^1.36.1",
    "@knime/hub-features": "^1.14.1",
    "@knime/kds-styles": "^0.0.4",
    "@knime/knime-ui-table": "0.5.23",
    "@knime/rich-text-editor": "^1.7.0",
    "@knime/styles": "^1.11.0",
    "@knime/ui-extension-renderer": "2.3.0",
    "@knime/utils": "^1.5.4",
    "@knime/virtual-tree": "^1.6.0",
    "@open-rpc/client-js": "^1.8.1",
    "@tiptap/extension-heading": "^2.26.1",
    "@tiptap/vue-3": "^2.26.1",
    "@vueuse/core": "^13.6.0",
    "@vueuse/integrations": "^13.6.0",
    "consola": "3.4.2",
    "dompurify": "^3.2.6",
    "events": "^3.3.0",
    "focus-trap": "^7.5.4",
    "fuse.js": "6.6.2",
    "kd-tree-javascript": "^1.0.3",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.2",
    "mitt": "^3.0.1",
    "motion": "^12.4.7",
    "pinia": "^2.3.0",
    "pixi.js": "^8.9.2",
    "raf-throttle": "2.0.6",
    "vue": "3.5.18",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.39.0",
    "@knime/eslint-config": "^9.0.2",
    "@knime/licenses": "^1.1.0",
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.50.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/kd-tree-javascript": "^1.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "9.22.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest-useragent-mock": "^0.1.1",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.5.1",
    "modern-normalize": "3.0.1",
    "mouse-helper": "^1.0.7",
    "npm-run-all2": "^8.0.4",
    "playwright-core": "^1.51.1",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "8.0.0",
    "prettier": "3.0.1",
    "rollup-plugin-sbom": "^2.0.3",
    "stylelint": "^16.18.0",
    "type-fest": "^4.40.0",
    "typescript": "^5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.4",
    "vue-tsc": "3.0.5"
  },
  "engines": {
    "node": "22.11.0",
    "npm": "please-use-pnpm"
  },
  "pnpm": {
    "overrides": {
      "@knime/knime-ui-table@0.5.22>@knime/components": "1.33.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "linkifyjs@<4.3.2": ">=4.3.2"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "libxmljs2"
    ],
    "auditConfig": {
      "ignoreGhsas": [
        "GHSA-v6h2-p8h4-qcjw"
      ]
    }
  },
  "packageManager": "pnpm@10.7.0"
}
