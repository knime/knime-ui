{
  "name": "knime-ui",
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && node buildtools/inline-html.mjs",
    "start": "nuxt start",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.mjs,.vue .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "audit": "check-audit --production",
    "test:unit": "jest",
    "test:integration": "node test/integration/run.js",
    "coverage": "jest --coverage",
    "sendcoverage": "cross-var sonar-scanner -Dsonar.login=\"$SONAR_LOGIN\" -Dsonar.password=\"$SONAR_PASSWORD\""
  },
  "dependencies": {
    "consola": "2.15.0",
    "lodash": "4.17.21",
    "nuxt": "2.15.4",
    "portal-vue": "2.1.7",
    "typeface-roboto": "0.0.75",
    "typeface-roboto-condensed": "0.0.75",
    "typeface-roboto-mono": "0.0.75",
    "vue-clickaway": "2.2.2",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@babel/preset-env": "7.12.17",
    "@codeceptjs/ui": "0.4.6",
    "@vue/test-utils": "1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "body-parser": "1.19.0",
    "codeceptjs": "3.0.7",
    "codeceptjs-assert": "0.0.5",
    "cross-var": "1.1.0",
    "dotenv": "10.0.0",
    "eslint": "7.5.0",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-vue": "7.10.0",
    "execa": "5.0.0",
    "jest": "26.1.0",
    "jest-junit": "6.4.0",
    "jest-transform-stub": "2.0.0",
    "jest-useragent-mock": "0.1.1",
    "jsdom": "16.3.0",
    "mocha": "^9.1.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi": "^1.1.3",
    "normalize.css": "8.0.1",
    "npm-audit-resolver": "2.3.1",
    "puppeteer": "10.1.0",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "20.0.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "vue-jest": "3.0.6",
    "vue-svg-loader": "0.11.0",
    "wait-on": "5.2.1"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "private": true
}
