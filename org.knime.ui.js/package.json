{
  "name": "knime-ui",
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-css && vite build",
    "serve": "vite preview",
    "postinstall": "npm run opensourcecredits -- --no-overwrite && npm run generate-css",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "ci:lint": "run-p lint:*",
    "audit": "check-audit --omit dev",
    "audit:resolve": "resolve-audit --omit dev",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "opensourcecredits": "npm run --prefix webapps-common opensourcecredits",
    "generate-css": "node ./buildtools/generateCSS.mjs"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "4.5.9",
    "@knime/ui-extension-service": "0.5.0",
    "fuse.js": "6.6.2",
    "gsap": "3.11.4",
    "lodash": "4.17.21",
    "mitt": "^3.0.0",
    "pagebuilder": "file:knime-js-pagebuilder/org.knime.js.pagebuilder",
    "raf-throttle": "2.0.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@knime/eslint-config": "7.1.5",
    "@types/node": "^18.14.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "8.35.0",
    "jest-useragent-mock": "^0.1.1",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "3.0.0-7",
    "npm-run-all": "4.1.5",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "8.0.0",
    "stylelint": "15.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "volta": {
    "node": "18.14.2"
  }
}
