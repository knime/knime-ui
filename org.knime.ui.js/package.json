{
  "name": "knime-ui",
  "private": true,
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build",
    "serve": "vite preview",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint --cache . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "format": "prettier --cache --write src/",
    "format:check": "prettier --cache --check src/",
    "ci:lint-format": "run-p lint:* format:check -c",
    "audit": "check-audit --omit dev",
    "audit:resolve": "resolve-audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "opensourcecredits": "npm run --prefix webapps-common opensourcecredits",
    "postinstall": "npm run opensourcecredits -- --no-overwrite",
    "add-husky": "cd .. && husky install ./org.knime.ui.js/.husky"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "4.5.9",
    "@knime/knime-ui-table": "0.3.43",
    "@knime/ui-extension-service": "^0.13.0",
    "@open-rpc/client-js": "^1.8.1",
    "@vueuse/core": "^9.13.0",
    "events": "^3.3.0",
    "fuse.js": "6.6.2",
    "gsap": "3.12.2",
    "lodash": "4.17.21",
    "mitt": "^3.0.0",
    "pagebuilder": "file:knime-js-pagebuilder/org.knime.js.pagebuilder",
    "raf-throttle": "2.0.6",
    "splitpanes": "^3.1.5",
    "vue": "^3.3.4",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "8.0.7",
    "@types/node": "^18.14.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "8.40.0",
    "husky": "^8.0.3",
    "jest-useragent-mock": "^0.1.1",
    "lint-staged": "^13.2.0",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "3.0.0-7",
    "npm-run-all": "4.1.5",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "8.0.0",
    "prettier": "3.0.1",
    "stylelint": "15.8.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.14.2"
  }
}
