{
  "name": "knime-ui",
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run generate-css && vue-cli-service build",
    "postinstall": "npm run opensourcecredits -- --no-overwrite && npm run generate-css",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:json",
    "lint:ci": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "lint:js": "eslint --ext .js,.mjs,.vue .",
    "lint:json": "eslint --ext .json --no-error-on-unmatched-pattern .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "audit": "check-audit --production",
    "audit:ci": "check-audit --production --json > target/audit.json || true",
    "audit:resolve": "resolve-audit --production",
    "test:unit": "vue-cli-service test:unit",
    "coverage": "npm run test:unit -- --coverage",
    "coverage:watch": "npm run test:unit -- --coverage --watch",
    "opensourcecredits": "npm run --prefix webapps-common opensourcecredits",
    "generate-css": "node ./buildtools/generateCSS.mjs"
  },
  "dependencies": {
    "@fontsource/roboto": "4.5.3",
    "@fontsource/roboto-condensed": "4.5.4",
    "@fontsource/roboto-mono": "4.5.3",
    "consola": "2.15.3",
    "core-js": "3.8.3",
    "focus-trap": "6.0.1",
    "focus-trap-vue": "0.0.6",
    "fuse.js": "6.5.3",
    "gsap": "3.8.0",
    "knime-ui-extension-service": "file:knime-ui-extension-service",
    "lodash": "4.17.21",
    "portal-vue": "2.1.7",
    "postcss-preset-env": "7.5.0",
    "raf-throttle": "2.0.5",
    "vue": "2.6.14",
    "vue-clickaway2": "2.3.2",
    "vue-router": "3.5.1",
    "vuex": "3.6.2",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@types/jest": "27.5.2",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-router": "5.0.8",
    "@vue/cli-plugin-unit-jest": "5.0.8",
    "@vue/cli-plugin-vuex": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/test-utils": "1.2.2",
    "@vue/vue2-jest": "^27.0.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "consola": "2.15.3",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "25.2.3",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-jsonc": "2.0.0",
    "eslint-plugin-vue": "7.20.0",
    "file-loader": "^6.2.0",
    "jest": "27.5.1",
    "jest-junit": "14.0.0",
    "jest-transform-stub": "2.0.0",
    "jest-useragent-mock": "0.1.1",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "2.3.1",
    "postcss-import": "14.1.0",
    "postcss-url": "10.1.3",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "20.0.0",
    "url-loader": "^4.1.1",
    "vue-svg-loader": "0.17.0-beta.2",
    "vue-template-compiler": "2.6.14"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "private": true,
  "volta": {
    "node": "14.20.0"
  }
}
