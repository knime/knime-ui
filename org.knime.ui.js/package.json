{
  "name": "knime-ui",
  "private": true,
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && vite build",
    "build:e2e": "vite build --mode e2e",
    "serve": "vite preview",
    "lint": "run-p 'lint:* --fix'",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "ci:lint-format": "run-p lint:* format:check -c",
    "audit": "pnpm audit --prod",
    "test:unit": "vitest",
    "test:e2e": "playwright test behavior/",
    "test:e2e:update": "playwright test behavior/ --update-snapshots",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test performance/",
    "coverage": "vitest run --coverage",
    "license-check": "license-check -c",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "add-husky": "cd .. && husky ./org.knime.ui.js/.husky",
    "sbom": "npx @cyclonedx/cyclonedx-npm --output-file bom.json --ignore-npm-errors --omit dev",
    "postinstall": "license-check -o licenses/used-packages.json"
  },
  "dependencies": {
    "@antfu/utils": "^9.1.0",
    "@floating-ui/vue": "^1.1.1",
    "@fontsource/roboto": "4.x",
    "@fontsource/roboto-condensed": "4.x",
    "@fontsource/roboto-mono": "4.x",
    "@knime/components": "^1.25.3",
    "@knime/hub-features": "^1.7.1",
    "@knime/knime-ui-table": "^0.5.22",
    "@knime/rich-text-editor": "^1.4.6",
    "@knime/styles": "^1.4.0",
    "@knime/ui-extension-renderer": "2.0.35",
    "@knime/utils": "^1.4.0",
    "@knime/virtual-tree": "^1.3.43",
    "@open-rpc/client-js": "^1.8.1",
    "@tiptap/extension-heading": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@types/kd-tree-javascript": "^1.0.3",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.11.0",
    "consola": "3.2.3",
    "dompurify": "^3.0.6",
    "events": "^3.3.0",
    "focus-trap": "^7.5.4",
    "fuse.js": "6.6.2",
    "kd-tree-javascript": "^1.0.3",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.2",
    "mitt": "^3.0.1",
    "motion": "^12.4.7",
    "pinia": "^2.3.0",
    "pixi.js": "^8.8.0",
    "raf-throttle": "2.0.6",
    "vue": "3.5.13",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.16.1",
    "@dotenvx/dotenvx": "^1.39.0",
    "@knime/eslint-config": "^9.0.1",
    "@knime/licenses": "^1.1.0",
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.50.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "chalk": "4.1.2",
    "eslint": "9.22.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest-useragent-mock": "^0.1.1",
    "jsdom": "^24.1.0",
    "lint-staged": "^13.2.0",
    "modern-normalize": "1.1.0",
    "npm-run-all": "4.1.5",
    "playwright-core": "^1.51.1",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "8.0.0",
    "prettier": "3.0.1",
    "stylelint": "^16.14.1",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.0.14",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.0.3",
    "vue-tsc": "2.2.0"
  },
  "engines": {
    "node": "22.11.0",
    "npm": "please-use-pnpm"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "libxmljs2"
    ]
  },
  "packageManager": "pnpm@10.7.0"
}
