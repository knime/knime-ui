{
  "name": "knime-ui",
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "nuxt && npm run opensourcecredits -- -- -- --no-overwrite",
    "build": "nuxt build && node buildtools/inline-html.mjs",
    "postinstall": "npm run opensourcecredits -- --no-overwrite",
    "start": "nuxt start",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:json",
    "lint:js": "eslint --ext .js,.mjs,.vue .",
    "lint:json": "eslint --ext .json --no-error-on-unmatched-pattern .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "audit": "check-audit --production",
    "audit:resolve": "resolve-audit --production",
    "test:unit": "jest",
    "coverage": "jest --coverage",
    "coverage:watch": "npm run test:unit -- --coverage --watch",
    "sendcoverage": "cross-var sonar-scanner -Dsonar.login=\"$SONAR_LOGIN\" -Dsonar.password=\"$SONAR_PASSWORD\"",
    "opensourcecredits": "npm run --prefix webapps-common opensourcecredits"
  },
  "dependencies": {
    "@fontsource/roboto": "4.5.3",
    "@fontsource/roboto-condensed": "4.5.4",
    "@fontsource/roboto-mono": "4.5.3",
    "consola": "2.15.3",
    "focus-trap": "^6.9.3",
    "focus-trap-vue": "^1.1.1",
    "fuse.js": "^6.5.3",
    "gsap": "3.8.0",
    "lodash": "4.17.21",
    "nuxt": "2.15.8",
    "portal-vue": "2.1.7",
    "raf-throttle": "^2.0.5",
    "vue-clickaway2": "2.3.2",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.0",
    "@vue/test-utils": "1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.3.1",
    "body-parser": "1.19.0",
    "cross-var": "1.1.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "25.2.3",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-jsonc": "2.0.0",
    "eslint-plugin-vue": "7.20.0",
    "execa": "5.1.1",
    "jest": "27.3.1",
    "jest-junit": "13.0.0",
    "jest-transform-stub": "2.0.0",
    "jest-useragent-mock": "0.1.1",
    "jsdom": "18.0.1",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "2.3.1",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "20.0.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "vue-jest": "3.0.7",
    "vue-svg-loader": "0.16.0",
    "wait-on": "5.2.1"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "private": true,
  "volta": {
    "node": "14.19.0"
  }
}
