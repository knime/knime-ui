{
  "name": "knime-ui",
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-css && vite build",
    "serve": "vite preview",
    "postinstall": "npm run opensourcecredits -- --no-overwrite && npm run generate-css",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.json",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "ci:lint": "run-p lint:*",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "audit": "check-audit --omit dev",
    "audit:resolve": "resolve-audit --omit dev",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "test:unit": "TZ='Europe/Berlin' jest",
    "coverage": "npm run test:unit -- --coverage --no-cache",
    "coverage:watch": "npm run test:unit -- --coverage --watch",
    "opensourcecredits": "npm run --prefix webapps-common opensourcecredits",
    "generate-css": "node ./buildtools/generateCSS.mjs"
  },
  "dependencies": {
    "@fontsource/roboto": "4.5.3",
    "@fontsource/roboto-condensed": "4.5.4",
    "@fontsource/roboto-mono": "4.5.3",
    "consola": "2.15.3",
    "fuse.js": "6.5.3",
    "gsap": "3.8.0",
    "knime-ui-extension-service": "file:knime-ui-extension-service",
    "lodash": "4.17.21",
    "portal-vue": "2.1.7",
    "raf-throttle": "2.0.5",
    "vue": "2.6.14",
    "vue-clickaway2": "2.3.2",
    "vue-router": "3.5.1",
    "vuex": "3.6.2",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "7.0.5",
    "@types/jest": "^28.1.8",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/test-utils": "1.2.2",
    "@vue/vue2-jest": "^28.1.0",
    "eslint": "8.27.0",
    "file-loader": "^6.2.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-junit": "14.0.0",
    "jest-transform-stub": "2.0.0",
    "jest-useragent-mock": "0.1.1",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "3.0.0-7",
    "npm-run-all": "4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-import": "14.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "7.8.1",
    "postcss-url": "10.1.3",
    "stylelint": "14.14.1",
    "url-loader": "^4.1.1",
    "vite": "^3.1.0",
    "vite-plugin-vue2": "^2.0.2",
    "vite-plugin-vue2-svg": "^0.3.0",
    "vue-template-compiler": "2.6.14"
  },
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "volta": {
    "node": "18.8.0"
  }
}
