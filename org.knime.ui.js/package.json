{
  "name": "knime-ui",
  "private": true,
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build",
    "serve": "vite preview",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint --cache . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "format": "prettier --cache --write src/",
    "format:check": "prettier --cache --check src/",
    "ci:lint-format": "run-p lint:* format:check -c",
    "audit": "check-audit --omit dev",
    "audit:resolve": "resolve-audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "license-check": "license-check -c",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "add-husky": "cd .. && husky install ./org.knime.ui.js/.husky",
    "sbom": "npx @cyclonedx/cyclonedx-npm --output-file bom.json --ignore-npm-errors --omit dev",
    "postinstall": "license-check -o licenses/used-packages.json"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.1",
    "@fontsource/roboto-condensed": "4.5.9",
    "@knime/components": "^1.0.10",
    "@knime/knime-ui-table": "^0.5.0",
    "@knime/rich-text-editor": "^1.2.0",
    "@knime/styles": "^1.0.6",
    "@knime/ui-extension-service": "0.38.0",
    "@knime/ui-extensions-renderer": "^1.0.14",
    "@knime/utils": "^1.0.6",
    "@open-rpc/client-js": "^1.8.1",
    "@types/kd-tree-javascript": "^1.0.3",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.11.0",
    "@ysx-libs/vue-virtual-tree": "^0.0.13",
    "consola": "3.2.3",
    "dompurify": "^3.0.6",
    "events": "^3.3.0",
    "focus-trap": "^7.5.4",
    "fuse.js": "6.6.2",
    "gsap": "3.12.2",
    "kd-tree-javascript": "^1.0.3",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.2",
    "mitt": "^3.0.1",
    "raf-throttle": "2.0.6",
    "vue": "3.4.21",
    "vue-router": "^4.1.6",
    "vue3-click-away": "1.2.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.16.1",
    "@knime/eslint-config": "^8.1.0",
    "@knime/licenses": "^1.0.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^2.0.3",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "8.56.0",
    "husky": "^8.0.3",
    "jest-useragent-mock": "^0.1.1",
    "jsdom": "^24.1.0",
    "lint-staged": "^13.2.0",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "3.0.0-RC.0",
    "npm-run-all": "4.1.5",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "8.0.0",
    "prettier": "3.0.1",
    "stylelint": "^16.1.0",
    "type-fest": "^4.11.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.6",
    "vite-plugin-vue-devtools": "^7.0.14",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^2.0.3",
    "vue-tsc": "2.0.6"
  },
  "engines": {
    "node": "20.x"
  },
  "volta": {
    "node": "20.11.0"
  }
}
