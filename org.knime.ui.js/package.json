{
  "name": "knime-ui",
  "version": "0.0.0-unused+see-pom.xml",
  "description": "KNIME HTML based UI",
  "author": "KNIME AG",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build",
    "serve": "vite preview",
    "postinstall": "npm run opensourcecredits -- --no-overwrite",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "ci:lint": "run-p lint:* format:check",
    "audit": "check-audit --omit dev",
    "audit:resolve": "resolve-audit --omit dev",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "jenkins:lint": "eslint --ext .js,.ts,.json . -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "jenkins:audit": "check-audit --omit dev --json > target/audit.json || true",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "opensourcecredits": "npm run --prefix webapps-common opensourcecredits",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepare": "cd .. && husky install ./org.knime.ui.js/.husky",
    "dummy": "npm run dev lint"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --cache --fix",
      "prettier --cache --write"
    ],
    "*.{css,vue}": [
      "stylelint --fix",
      "prettier --cache --write"
    ]
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "4.5.9",
    "@knime/ui-extension-service": "0.9.0",
    "@tiptap/extension-text-align": "^2.0.2",
    "@tiptap/extension-underline": "^2.0.2",
    "@tiptap/pm": "^2.0.2",
    "@tiptap/starter-kit": "^2.0.2",
    "@tiptap/vue-3": "^2.0.2",
    "fuse.js": "6.6.2",
    "gsap": "3.11.4",
    "lodash": "4.17.21",
    "mitt": "^3.0.0",
    "pagebuilder": "file:knime-js-pagebuilder/org.knime.js.pagebuilder",
    "raf-throttle": "2.0.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "8.0.1",
    "@types/node": "^18.14.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "8.40.0",
    "husky": "^8.0.3",
    "jest-useragent-mock": "^0.1.1",
    "lint-staged": "^13.2.0",
    "modern-normalize": "1.1.0",
    "npm-audit-resolver": "3.0.0-7",
    "npm-run-all": "4.1.5",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "8.0.0",
    "prettier": "2.8.8",
    "stylelint": "15.6.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "volta": {
    "node": "18.14.2"
  }
}
